package com.learnnow.dto;

import java.time.LocalDate;
import java.time.LocalDateTime;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
public class UserEntityResponseDTO extends BaseDTO {
	    private String firstName;
	    private String lastName;
	    private String email;
	    private LocalDate dob;
	    private String role;
		public UserEntityResponseDTO(Long id, LocalDate creationDate, LocalDateTime updatedOn, String firstName,
				String lastName, String email, LocalDate dob, String role) {
			super(id, creationDate, updatedOn);
			this.firstName = firstName;
			this.lastName = lastName;
			this.email = email;
			this.dob = dob;
			this.role = role;
		}
		
	    
	    
	    
	  
}
